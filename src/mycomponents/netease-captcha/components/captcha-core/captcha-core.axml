<template name="captchaTip"><view class="yidun-tips {{ captchaTip && verifyStatus && verifyStatus !== VERIFY_STATUS.DOING ? 'yidun-tips--' + verifyStatus : ''}}" catchTap="resetIfNeed"><view class="yidun-tips__icon"></view><view hover-class="yidun-tips__content--hover"><text>{{ captchaTip }}</text></view></view></template><view class="yidun-popup {{ 'yidun-popup--' + theme }} {{ popupVisible ? 'active' : '' }}"><view class="yidun-popup__mask" style="{{ 'opacity:' + maskOpacity }}"></view><view class="yidun-modal__wrap"><view class="yidun-popup__modal" animation="{{modalAnimationData}}" style="{{ widthIsNotAuto ? 'width:' + popWidth : '' }}"><view class="yidun-modal__header" style="{{ 'height: ' + customStyles.popTitle.height }}"><view><text>{{ langPkg.POPUP_TITLE }}</text></view><button class="yidun-popup__close" onTap="closeDialog"><view class="yidun-popup__icon"></view></button></view><view class="yidun-modal__body" style="{{ 'padding: ' + customStyles.popPadding }}"><view class="yidun-captcha__wrap {{ loadInfo.status !== 'done' ? 'yidun-captcha__wrap--loading' : '' }} query-wrap-{{ uuid }}" style="{{ 'width: ' + (widthIsNotAuto ? 'auto' : config.width) + ';min-width: ' + minWidth + 'px;' }}"><captcha-jigsaw a:if="{{captchaType === CAPTCHA_TYPE.JIGSAW}}" jigsawCaptchaClass="yidun-captcha" loadInfo="{{loadInfo}}" onImageLoad="finishLoad" onVerify="onVerifyCaptcha" wrapWidth="{{width}}" verifyStatus="{{verifyStatus}}" context="{{context}}"><template is="captchaTip" data="{{...customStyles.controlBar, captchaTip, verifyStatus, resetIfNeed}}"></template></captcha-jigsaw><captcha-point a:elif="{{[CAPTCHA_TYPE.POINT, CAPTCHA_TYPE.ICON_POINT].indexOf(captchaType) !== -1}}" pointCaptchaClass="yidun-captcha" loadInfo="{{loadInfo}}" captchaType="{{captchaType}}" onImageLoad="finishLoad" onVerify="onVerifyCaptcha" verifyStatus="{{verifyStatus}}" context="{{context}}"><template is="captchaTip" data="{{...customStyles.controlBar, captchaTip, verifyStatus, resetIfNeed}}"></template></captcha-point><captcha-load loadCaptchaClass="yidun-captcha--load" status="{{ loadInfo.status }}" context="{{context}}"><template is="captchaTip" data="{{...customStyles.controlBar, captchaTip}}"></template></captcha-load><button a:if="{{ loadInfo.status !== 'loading' && verifyStatus !== VERIFY_STATUS.SUCCESS && verifyStatus !== VERIFY_STATUS.DOING }}" disabled="{{ verifyStatus === VERIFY_STATUS.DOING || verifyStatus === VERIFY_STATUS.MAX_ERROR }}" class="yidun-captcha__refresh" catchTap="refresh"></button></view></view></view></view></view>